{
  "name": "nuls-api-v2",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/api.cjs",
  "module": "src/api.js",
  "types": "src/api.d.ts",
  "exports": {
    ".": {
      "require": "./dist/api.cjs",
      "import": "./src/api.js"
    }
  },
  "scripts": {
    "test": "node test/test.js",
    "build:cjs": "babel src --out-dir dist --out-file-extension .cjs",
    "build": "yarn build:cjs && cp src/api.js src/api.ts && tsc -d --emitDeclarationOnly --outDir temp --lib es2016,dom src/api.ts && rm src/api.ts && cp temp/api.d.ts src/api.d.ts && rm -rf temp",
    "postinstall":"yarn build"
  },
  "dependencies": {
    "axios": "1.7.7",
    "bignumber.js": "^9.1.2",
    "bs58": "^6.0.0",
    "crypto-js": "^4.2.0",
    "js-sha3": "^0.9.3",
    "node-localstorage": "^3.0.5",
    "nuls-sdk-js": "^2.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.0",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "typescript": "^5.7.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
