{
  "name": "nuls-api-v2",
  "version": "1.0.0",
  "license": "MIT",
  "main": "lib/api.js",
  "module": "src/api.js",
  "types": "src/api.d.ts",
  "exports": {
    ".": {
      "import": "./src/api.js",
      "require": "./lib/api.js"
    }
  },
  "scripts": {
    "test": "node test.js",
    "build:cjs": "babel src --out-dir lib",
    "build": "cp src/api.js src/api.ts && tsc -d --emitDeclarationOnly --outDir dist --lib es2016,dom src/api.ts && rm src/api.ts && cp dist/api.d.ts src/api.d.ts && rm -rf dist"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bignumber.js": "^9.1.2",
    "bs58": "^6.0.0",
    "crypto-js": "^4.2.0",
    "js-sha3": "^0.9.3",
    "node-localstorage": "^3.0.5",
    "nuls-sdk-js": "^2.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.0",
    "typescript": "^5.7.2"
  }
}